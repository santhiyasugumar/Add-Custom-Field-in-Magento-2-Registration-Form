<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="customer">
        <field name="gst_number" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="sap_customer_code" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="company_name" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="designation" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="contact_number" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="countryid" formElement="select">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="stateid" formElement="select">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="is_custom_state" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="custom_state" formElement="input">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="interested_area" formElement="multiselect">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
        <field name="is_agreed" formElement="checkbox">
            <settings>
                <visible>false</visible>
            </settings>
        </field>
    </fieldset>
</form>
